# Pollinations settings
POLLINATIONS_MODEL=openai-large
POLLINATIONS_SYSTEM_PROMPT=You are a concise assistant.
POLLINATIONS_TIMEOUT_MS=45000
POLLINATIONS_MIN_INTERVAL_MS=15000

# Telegram (optional)
TELEGRAM_BOT_TOKEN=
TELEGRAM_ALLOWED_USER_IDS=
TELEGRAM_COMMAND_PREFIX=
TELEGRAM_LOCK_PATH=/tmp/openai-agent-telegram.lock

# Browser adapter
BROWSER_HEADLESS=true
BROWSER_TIMEOUT_MS=30000
BROWSER_MAX_CHARS=3000

LOG_LEVEL=info
